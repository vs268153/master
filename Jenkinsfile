    agent any
    stages {
        stage('build') {
            steps {
                echo 'This is my first build'
            }
        }
        stage('test') {
            steps {
                echo 'This is my first test'
            }
        }
        stage('deploy') {
            steps {
                echo 'This is my first deploy'
            }
        }
    }
 }
